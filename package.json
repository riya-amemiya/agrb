{
	"name": "agrb",
	"version": "1.5.0",
	"description": "Interactive CLI to rebase a branch onto another safely (cherry-pick or linear rebase)",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/riya-amemiya/agrb.git"
	},
	"homepage": "https://github.com/riya-amemiya/agrb#readme",
	"bugs": {
		"url": "https://github.com/riya-amemiya/agrb/issues"
	},
	"scripts": {
		"build": "tsc --noEmit && vite build",
		"dev": "vite build --watch",
		"test": "biome check .",
		"eslint": "eslint src",
		"lint": "biome check . --write",
		"ci": "biome ci .",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"git",
		"rebase",
		"cherry-pick",
		"cli",
		"ink",
		"github"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"ag-toolkit": "0.4.0",
		"ink": "6.6.0",
		"ink-select-input": "6.2.0",
		"ink-spinner": "5.0.0",
		"react": "19.2.3",
		"simple-git": "3.30.0",
		"umt": "2.11.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@sindresorhus/tsconfig": "8.1.0",
		"@types/bun": "1.3.5",
		"@types/react": "19.2.8",
		"@typescript-eslint/eslint-plugin": "8.53.0",
		"@typescript-eslint/parser": "8.53.0",
		"chalk": "5.6.2",
		"eslint": "9.39.2",
		"eslint-plugin-unicorn": "62.0.0",
		"ink-testing-library": "4.0.0",
		"ts-node": "10.9.2",
		"typescript": "5.9.3",
		"vite": "7.3.1"
	}
}
